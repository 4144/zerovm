=====================================================================
== "file input/output" example with zrt library usage
== i/o switches
=====================================================================
Output = samples/zrt/file_stat/file_stat.output.data
OutputMax = 1024000
OutputMaxGet = 1024000
OutputMaxGetCnt = 1024
OutputMaxPut = 1024000
OutputMaxPutCnt = 1024

== this options specifies output file loading method: 0 - loaded, 1 - mapped
OutputMode = 1

UserLog = samples/zrt/file_stat/file_stat.user.log
UserLogMax = 65536
UserLogMaxGet = 65536

=====================================================================
== switches for zerovm. some of them used to control nexe, some
== for the internal zerovm needs
=====================================================================
Version = 11nov2011
Log = samples/zrt/file_stat/file_stat.zerovm.log
Report = samples/zrt/file_stat/file_stat.report.log
Nexe = samples/zrt/file_stat/file_stat.nexe
NexeMax = 10000000
SyscallsMax = 3333
SetupCallsMax = 99999

== this option can be disabled if user code does not use date/time functions
==TimeStamp = 12345

== this option forces zerovm cut available for user memory to specified amount
==MemMax = 10240000

