=====================================================================
== copy data from the input channel to the output channel
=====================================================================
Channel = tcp:1:, /dev/stdin, 0, 0, 0x100000000, 0x100000000, 0, 0
Channel = /dev/stdout, /dev/stdout, 0, 0, 0, 0, 0x100000000, 0x100000000
Channel = /dev/null, /dev/stderr, 0, 0, 0, 0, 65536, 65536

=====================================================================
== switches for zerovm. some of them used to control nexe, some
== for the internal zerovm needs
=====================================================================
Version = 20140509
Program = copy.nexe
Memory = 0x2000000, 0
Timeout = 120
NameServer = udp:127.0.0.1:54321
Node = 2

